{
  "extensions": ["esbenp.prettier-vscode", "dbaeumer.vscode-eslint", "johnpapa.vscode-peacock"],
  "postCreateCommand": "zsh -ic 'npm i && npm run build'",
  "dockerFile": "Dockerfile",
  "context": "..",
  "workspaceMount": "/workspace",
  "settings": {
    "peacock.remoteColor": "#30355c",
    "files.autoSave": "onFocusChange",
    "editor.tabSize": 2
  }
}
